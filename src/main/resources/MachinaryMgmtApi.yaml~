  - url: 'http://localhost:8080/api/v1'
    description: Local development server


paths:
  /equipment:
    get:
      summary: Get all equipment
      operationId: getAllEquipment
      parameters:
        - name: page
          in: query
          schema:
            type: integer
            default: 0
        - name: size
          in: query
          schema:
            type: integer
            default: 10
